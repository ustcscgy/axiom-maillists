+\documentclass{article}
+\usepackage{axiom}
+\begin{document}
+\title{\$SPAD/src/input besselk.input}
+\author{Timothy Daly}
+\maketitle
+\begin{abstract}
+\end{abstract}
+\eject
+\tableofcontents
+\eject
+<<*>>=
+)spool besselk.output
+)set message test on
+)set message auto off
+)clear all
+ 
+--S 1 of 4
+D(besselK(a,x),x)
+--R
+--R        - besselK(a + 1,x) - besselK(a - 1,x)
+--R   (1)  -------------------------------------
+--R                          2
+--R                                                     Type: Expression Integer
+--E 1
+
+--S 2 of 4
+D(besselK(a,x),a)
+--R
+--R   (2)  besselK  (a,x)
+--R               ,1
+--R                                                     Type: Expression Integer
+--E 2
+
+--S 3 of 4
+integrate(D(besselK(a,x),a),a)
+--R
+--R   (3)  besselK(a,x)
+--R                                          Type: Union(Expression Integer,...)
+--E 3
+
+--S 4 of 4
+limit(D(besselK(a,x),a),a=1/2)
+--R
+--R   (4)  "failed"
+--R                                                    Type: Union("failed",...)
+--E 4
+)spool 
+)lisp (bye)
+ 
+@
+\eject
+\begin{thebibliography}{99}
+\bibitem{1} nothing
+\end{thebibliography}
+\end{document}
+



