    u:= SUFFIX(s.(0),u)
    for i in 1..MAXINDEX s repeat
      if not (DIGITP s.i or ALPHA_-CHAR_-P s.i or ((c := s.i) = char '?) 
!       or (c = char '!)) then SUFFIX('__,u)
      u:= SUFFIX(s.i,u)
    INTERN u
   
