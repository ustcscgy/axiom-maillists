+v.3 := 99
+--R 
+--R
+--R   (5)  99
+--R                                                        Type: PositiveInteger
+--E 5
+
+--S 6 of 11
+v
+--R 
+--R
+--R   (6)  [1,2,99,4,5]
+--R                                                         Type: Vector Integer
+--E 6
+
+--S 7 of 11
+5 * v
+--R 
+--R
+--R   (7)  [5,10,495,20,25]
+--R                                                         Type: Vector Integer
+--E 7
+
+--S 8 of 11
+v * 7
+--R 
+--R
+--R   (8)  [7,14,693,28,35]
+--R                                                         Type: Vector Integer
+--E 8
+
+--S 9 of 11
+w : VECTOR INT := vector([2,3,4,5,6])
+--R 
+--R
+--R   (9)  [2,3,4,5,6]
+--R                                                         Type: Vector Integer
+--E 9
+
