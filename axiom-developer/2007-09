+--R 
+--R
+--R           2
+--R   (7)  60x  - 46x + 8
+--R                                        Type: UnivariatePolynomial(x,Integer)
+--E 7
+
+--S 8 of 35
+gcd(p,q)
+--R 
+--R
+--R          2
+--R   (8)  9x  - 6x + 1
+--R                                        Type: UnivariatePolynomial(x,Integer)
+--E 8
