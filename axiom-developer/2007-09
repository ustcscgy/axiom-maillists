TK_XINCLUDES=3D
TCL_LIB_SPEC=3D-L/usr/lib64 -ltcl8.4${TCL_DBGX}
TCL_DL_LIBS=3D-ldl
TCL_LIBS=3D-ldl -lpthread  -lm

NOTIFY=3Dyes
CC=3Dgcc
CFLAGS=3D-march=3Dathlon64 -O2 -Wall -DVOL=3Dvolatile -fsigned-char -pipe=
 -O3 -fomit-frame-pointer  -I$(GCLDIR)/o
