+nevens := (0..) by -2
+--R 
+--R
+--R   (2)  0.. by - 2
+--R                                    Type: UniversalSegment NonNegativeInteger
+--E 2
+
+--S 3 of 9
+useg: UniversalSegment(Integer) := 3..10
+--R 
+--R
+--R   (3)  3..10
+--R                                               Type: UniversalSegment Integer
+--E 3
