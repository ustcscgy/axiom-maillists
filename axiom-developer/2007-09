	    posrcprefix=3D'../'; \
	  else \
	    posrcprefix=3D"../"; \
	  fi; \
	  rm -f SRC-POTFILES-t SRC-POTFILES \
	    && (sed -e '/^#/d' \
	            -e '/^[ 	]*$/d' \
		    -e "s@.*@	$posrcprefix& \\\\@" < ./SRC-POTFILES.in \
