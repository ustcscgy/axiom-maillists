index f4738b5..4c44625 100644
--- a/src/algebra/fname.spad.pamphlet
+++ b/src/algebra/fname.spad.pamphlet
@@ -203,20 +203,20 @@ writable? "/tmp/DoesNotExist"
 --R 
 --R
 --R   (17)  true
---R                                                             Type: Boolean
+--R                                                                Type: Boolean
 --E 17
 
 --S 18 of 18
 fn := new(objdir, "xxx", "yy") 
 --R 
 --R
---R   (18)  "/tmp/xxx1419.yy"
+--I   (18)  "/tmp/xxx1419.yy"
 --R                                                               Type: FileName
 --E 18
 )spool
 )lisp (bye)
 @
-<<Filename.help>>=
+<<FileName.help>>=
 ====================================================================
 FileName examples
 ====================================================================
