TK_LIBRARY=3D/usr/lib64/tk8.4
TCL_LIBRARY=3D/usr/lib64/tcl8.4
TK_XINCLUDES=3D
TK_INCLUDE=3D-I/usr/lib64/../include
TCL_INCLUDE=3D-I/usr/lib64/../include
