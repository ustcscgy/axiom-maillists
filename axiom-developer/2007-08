- 
-; OPT_EXPR:     <'*' +(STAR OPT_EXPR #1) / REPEATOR> +(OPTIONAL #1) ;
- 
-(DEFUN PARSE-OPT_EXPR NIL
-  (AND (OPTIONAL (OR (AND (MATCH-ADVANCE-STRING "*")
-                          (PUSH-REDUCTION 'PARSE-OPT_EXPR
-                                          (CONS 'STAR
-                                                (CONS '|OPT_EXPR|
-                                                      (CONS (POP-STACK-1) NIL)))))
-                     (PARSE-REPEATOR)))
