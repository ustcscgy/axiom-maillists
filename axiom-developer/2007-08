-; TEST:     IDENTIFIER (    '{' <SEXPR*>! '}'
-; +(=(INTERN (STRCONC META_PREFIX #2)) -#1)
-; / +(=(INTERN (STRCONC META_PREFIX #1))))        .(SETQ TRAPFLAG T)
-; / STRING  +(MATCH-ADVANCE-STRING #1)            .(SETQ TRAPFLAG T)
