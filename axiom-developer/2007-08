-                                      (CONS (CONS 'MATCH-ADVANCE-STRING
-                                                  (CONS (POP-STACK-1) NIL))
-                                            (CONS (CONS 'MUST (CONS (NTH-STACK 1) NIL))
-                                                  NIL))))))
-       (PUSH-REDUCTION 'PARSE-REPEATOR
-                       (CONS 'STAR (CONS 'REPEATOR (CONS (POP-STACK-1) NIL))))))
- 
