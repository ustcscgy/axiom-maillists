-      slot = slot->nextViewport;
-    };  /* while */
-    
-    if (keepLooking) {   /* if  1 => slots not read, read from spad */
-#ifdef DEBUG
-      fprintf(stderr,"viewman: still looking\n");
-#endif
-      viewType = get_int(spadSock);
-      if (viewType == -1) goodbye(-1);
