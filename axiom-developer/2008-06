-		  if (graphStateArray[i].centerY > maxDelta)
-		    graphStateArray[i].centerY = maxDelta;
-		  else if (graphStateArray[i].centerY < -maxDelta)
-		    graphStateArray[i].centerY = maxDelta;
-		}
-	      }	/* graph showing or selected */
