-/*
- * Given 2 file pointers, this function copies file ifp to file ofp
- */
-
-static void
-filecopy(FILE * ifp, FILE * ofp)
-{
-
-  int c;
-
-  while ((c = getc(ifp)) != EOF)
-    putc(c, ofp);
-}
