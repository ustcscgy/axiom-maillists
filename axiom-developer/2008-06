-    fprintf(stderr,"Ran out of memory (malloc) trying to create a viewport.\n");
-    sleep(5);
-    exitWithAck(RootWindow(dsply,scrn),Window,-1);
-  }
-
-#ifdef DEBUG
-  fprintf(stderr,"view2d: Made a viewport\n");
-#endif
