-		       ptY-halfSize,aList->pointSize,aList->pointSize,
-		       0,360*64, dFlag);
-                      
-          } /* if points on */
-          for (ii=0, aPoint=aList->listOfPoints; 
-	       ii<aList->numberOfPoints;
-	       ++ii, ++tempXpt, ++aPoint) {
-            ptX1 = tempXpt->x;
-            ptY1 = tempXpt->y;
