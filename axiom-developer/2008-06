-                    graphArray[i].unitX,graphArray[i].unitY);
-            fprintf(viewDataFile,"%d\n",graphArray[i].numberOfLists);
-            for (j=0,aList=graphArray[i].listOfListsOfPoints;
-                 j<graphArray[i].numberOfLists;
-                 j++, aList++) {
