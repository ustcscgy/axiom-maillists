-    GSetForeground(gc1, (float) psWhite, dFlag);
-    GSetBackground(gc1, (float) psBlack, dFlag);
-    if (GDrawImageString(gc1, viewport->viewWindow, Ix, Iy,
-                         Istr, strlen(Istr), dFlag) == psError)
-        return (psError);
