-			       (float)monoColor((int)(aPoint->hue)),
-			       dFlag);
-              } else {
-                GSetForeground(globalGC1,
-			       (float)XSolidColor((int)(aPoint->hue),
-						  (int)(aPoint->shade)),
-			       dFlag);
-              }
-            }
