-	  
-	case ClientMessage:
-	  if (event->xclient.data.l[0] == wm_delete_window) {
-	    	  goodbye(-1);
-	  }
-	  else {
-	    fprintf(stderr,"Unknown Client Message ...\n");
-	  }
-	  break;
