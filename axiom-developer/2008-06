-  if (vW) {
-    titleSizeHints.flags  = PPosition | PSize;
-    titleSizeHints.x      = vX;
-    titleSizeHints.y      = vY;
-    titleSizeHints.width  = vW;
-    titleSizeHints.height = vH;
-  } else {
-    titleSizeHints.flags  = PSize;
-    titleSizeHints.width  = viewWidth;
-    titleSizeHints.height = viewHeight;
-  }
-
