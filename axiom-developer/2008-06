-	  if (followMouse==yes) {
-	    followMouse = no;
-	    toggleReady = yes;
-	    checkButton = no;
-	    drawViewport(Xoption);
-	  } else {
-	    followMouse = no;
-	    toggleReady = yes;
-	    checkButton = no;
-	  }
-	  break;
-	  
-	case LeaveNotify:
-	  /*
-	    We still follow the mouse when we leave the pots.
-	    */
-	  /* 	  
-		  followMouse = no;
-		  toggleReady = yes; 
-		  checkButton = no; 
-		  */
-
-	  break;
-	  
-	case ButtonPress:
-	  if (whichWindow == viewport->viewWindow) {
-	    
-	    /* mouse clicked on viewport */
-
-	    switch (((XButtonEvent *)event)->button) {
-	    case Button3:
