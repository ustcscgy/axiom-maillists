-                if (mono) {
-                  GSetForeground(globalGC1,
-			 (float)monoColor((int)(aList->lineColor-1)/5),
-			 dFlag);
-                } else {
-                  GSetForeground(globalGC1,
-			 (float)XSolidColor((int)(aList->lineColor-1)/5,
-					    (int)((aList->lineColor-1)%5)/2),
-				 dFlag);
-                }
-	      } /* if X */
