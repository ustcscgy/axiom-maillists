-			  viewport->titleWindow, viewport->title) == psError)
-           return(-1);  	/* concat script & proc into axiom2d.ps */
-          break;
-
-        } /* switch on ii */
-      }  /* if thingsToWrite >> ii */
-    }  /* for ii */
-
-    return(0);
-  }    /* else create directory okay */
-
-}
