-    graphArray[i].key = 0;   /* 0 means no graph */
-    for (j=0,llPtr=graphArray[i].listOfListsOfPoints; 
-	 j<graphArray[i].numberOfLists; j++,llPtr++) 
-      free(llPtr->listOfPoints);
-    free(llPtr);
-    free(xPointsArray[i].xPoint);
-  } else {
-  }
-
-}
