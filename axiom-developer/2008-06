-
-#ifdef DEBUG
-  fprintf(stderr,"view2d: Tidying up and exiting\n");
-#endif
-  PSClose(); /* free PS file and data structure space */
-
-  XFreeGC(dsply,globalGC1);
-  XFreeGC(dsply,globalGC2);
-  XFreeGC(dsply,globGC);
