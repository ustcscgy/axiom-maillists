-#ifdef DEBUG
-        fprintf(stderr,"viewman: viewing a tube\n");
-#endif
-        if (viewCommand == makeViewport) 
-          forkView3D(viewTubeType);
-	else 
-          funView3D(viewCommand);
-        
-        break;
-        
