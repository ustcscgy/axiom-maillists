-          graphList         = aGraph;
-        }
-        break;
-        
-      case view2DType:
-#ifdef DEBUG
-	fprintf(stderr,"viewman: forking 2D\n");
-#endif
-        if (viewCommand == makeViewport) {
-          forkView2D();
-        } else {
-          funView2D(viewCommand);
-        }
-        break;
-        
-      }   /* switch on viewType */
-    }   /* if (keepLooking) */ 
-  }   /* while (1) */
-}
