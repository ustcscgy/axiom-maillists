-	  GSetForeground(graphGC,(float)foregroundColor,Xoption);
-	  GSetBackground(graphGC,(float)backgroundColor,Xoption);
-	}
-	strlength = strlen((cp->buttonQueue[i]).text);
-	GDrawImageString(graphGC,cp->controlWindow,
