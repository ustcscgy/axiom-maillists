   NSigList:= nil
   if PrincipalAncestor=nil then count:= 6 else count:= SIZE PrincipalAncestor
@@ -26,23 +110,7 @@ mkCategory(domainOrPackage,sigList,attList,domList,PrincipalAncestor) ==
