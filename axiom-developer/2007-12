+<h5>Dyson's Integral</h5>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mfrac>
+      <m:mn>1</m:mn>
+      <m:msup>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mrow>
+         <m:mn>2</m:mn>
+         <m:mi>&#x03C0;</m:mi>
+        </m:mrow>
+        <m:mo>)</m:mo>
+       </m:mrow>
+       <m:mi>n</m:mi>
+      </m:msup>
+     </m:mfrac>
+     <m:mrow>
+      <m:msub>
