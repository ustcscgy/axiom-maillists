+      <m:mi>b</m:mi>
+     </m:mrow>
+    </m:msup>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
