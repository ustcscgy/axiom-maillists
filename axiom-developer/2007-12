+        <m:mo>+</m:mo>
+        <m:mi>a</m:mi>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi>z</m:mi>
+        <m:mo>+</m:mo>
+        <m:mi>b</m:mi>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mfrac>
