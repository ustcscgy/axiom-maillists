+       <m:mi>k</m:mi>
+       <m:mo>=3D</m:mo>
+       <m:mn>0</m:mn>
+      </m:mrow>
+      <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+     </m:munderover>
+     <m:mo>(</m:mo>
