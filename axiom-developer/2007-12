+          <m:mi>k</m:mi>
+         </m:mrow>
+         <m:mo>)</m:mo>
+        </m:mrow>
+        <m:mn>2</m:mn>
+       </m:msup>
+      </m:mfrac>
+     </m:mrow>
+     <m:mo>)</m:mo>
+    </m:mrow>
+   </m:mrow>
+   <m:mo>,</m:mo>
+  </m:mrow>
+ </m:math>
