+ <body>
+<<page head>>
+  <div align=3D"center">
+   <a href=3D"http://dlmf.nist.gov">
+    Digital Library of Mathematical Functions
+   </a><br/>
+   The Gamma Function -- Infinite Products
+  </div>
+  <hr/>
+<h3>Infinite Products</h3>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mi>z</m:mi>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:munder>
+      <m:mo movablelimits=3D"false">lim</m:mo>
+      <m:mrow>
+       <m:mi>k</m:mi>
+       <m:mo>&#x2192;</m:mo>
+       <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+      </m:mrow>
+     </m:munder>
+     <m:mfrac>
+      <m:mrow>
+       <m:mrow>
+       <m:mi>k</m:mi>
+       <m:mi mathvariant=3D"normal">!</m:mi>
+      </m:mrow>
+      <m:msup>
+       <m:mi>k</m:mi>
+       <m:mi>z</m:mi>
+      </m:msup>
+     </m:mrow>
+     <m:mrow>
+      <m:mi>z</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi>z</m:mi>
+        <m:mo>+</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+      <m:mi mathvariant=3D"normal">&#x22EF;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi>z</m:mi>
+        <m:mo>+</m:mo>
+        <m:mi>k</m:mi>
+       </m:mrow>
+       <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+     </m:mfrac>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"right">
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>z</m:mi>
+   <m:mo>&#x2260;</m:mo>
+   <m:mrow>
+    <m:mn>0</m:mn>
+    <m:mo>,</m:mo>
+    <m:mrow>
+     <m:mo>-</m:mo>
+     <m:mn>1</m:mn>
+    </m:mrow>
+    <m:mo>,</m:mo>
+    <m:mrow>
+     <m:mo>-</m:mo>
+     <m:mn>2</m:mn>
+    </m:mrow>
+    <m:mo>,</m:mo>
+    <m:mi mathvariant=3D"normal">&#x2026;</m:mi>
+   </m:mrow>
+  </m:mrow>
+ </m:math>,
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mfrac>
+     <m:mn>1</m:mn>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mi>z</m:mi>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mfrac>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:mi>z</m:mi>
+     <m:msup>
+      <m:mi mathvariant=3D"normal">&#x2147;</m:mi>
+      <m:mrow>
+       <m:mi>&#x03B3;</m:mi>
+       <m:mi>z</m:mi>
+      </m:mrow>
+     </m:msup>
+     <m:mrow>
+      <m:munderover>
+       <m:mo movablelimits=3D"false">&#x220F;</m:mo>
+       <m:mrow>
+        <m:mi>k</m:mi>
+        <m:mo>=3D</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+       <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+      </m:munderover>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mn>1</m:mn>
+        <m:mo>+</m:mo>
+        <m:mfrac>
+         <m:mi>z</m:mi>
+         <m:mi>k</m:mi>
+        </m:mfrac>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+      <m:msup>
+       <m:mi mathvariant=3D"normal">&#x2147;</m:mi>
+       <m:mrow>
+        <m:mo>-</m:mo>
