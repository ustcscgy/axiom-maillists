+         <m:mi>z</m:mi>
+         <m:mi>k</m:mi>
+        </m:mfrac>
+       </m:mrow>
+      </m:msup>
+     </m:mrow>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
