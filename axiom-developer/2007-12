+     <m:mrow>
+      <m:none/>
+      <m:mo>&lt;</m:mo>
+      <m:mi>&#x03C0;</m:mi>
+     </m:mrow>
+     <m:mo>)</m:mo>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math> and=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mn>1</m:mn>
+   <m:mo>&lt;</m:mo>
+   <m:mi>c</m:mi>
+   <m:mo>&lt;</m:mo>
+   <m:mn>2</m:mn>
+  </m:mrow>
+ </m:math>.
+</p>
