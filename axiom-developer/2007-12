+    <m:mrow>
+     <m:mrow>
+      <m:mo>-</m:mo>
+      <m:mi>&#x03B3;</m:mi>
+     </m:mrow>
+     <m:mo>+</m:mo>
+     <m:mrow>
+      <m:munderover>
