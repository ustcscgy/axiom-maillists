+             <m:mo>+</m:mo>
+             <m:mn>1</m:mn>
+            </m:mrow>
+            <m:mo>)</m:mo>
+           </m:mrow>
+          </m:mrow>
+          <m:mo>,</m:mo>
+          <m:mrow>
