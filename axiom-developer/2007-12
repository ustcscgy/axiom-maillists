+<h4>Gamma Function</h4>
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mn>1</m:mn>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mn>1</m:mn>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:mi>n</m:mi>
+       <m:mo>+</m:mo>
+       <m:mn>1</m:mn>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:mi>n</m:mi>
+     <m:mi mathvariant=3D"normal">!</m:mi>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mo>&#x2223;</m:mo>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+        <m:mi>y</m:mi>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>&#x2223;</m:mo>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:msup>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mfrac>
+       <m:mi>&#x03C0;</m:mi>
+       <m:mrow>
+        <m:mi>y</m:mi>
+        <m:mrow>
+         <m:mi>sinh</m:mi>
+         <m:mrow>
+          <m:mo>(</m:mo>
+          <m:mrow>
+           <m:mi>&#x03C0;</m:mi>
+           <m:mi>y</m:mi>
+          </m:mrow>
+          <m:mo>)</m:mo>
+         </m:mrow>
+        </m:mrow>
+       </m:mrow>
+      </m:mfrac>
+      <m:mo>)</m:mo>
+     </m:mrow>
+     <m:mfrac bevelled=3D"true">
+      <m:mn>1</m:mn>
+      <m:mn>2</m:mn>
+     </m:mfrac>
+    </m:msup>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mstyle displaystyle=3D"false">
+         <m:mfrac>
+          <m:mn>1</m:mn>
+          <m:mn>2</m:mn>
+         </m:mfrac>
+        </m:mstyle>
+        <m:mo>+</m:mo>
+        <m:mrow>
+         <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+         <m:mi>y</m:mi>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mstyle displaystyle=3D"false">
+         <m:mfrac>
+          <m:mn>1</m:mn>
+          <m:mn>2</m:mn>
+         </m:mfrac>
+        </m:mstyle>
+        <m:mo>-</m:mo>
+        <m:mrow>
+         <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+         <m:mi>y</m:mi>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:msup>
+     <m:mrow>
+      <m:mo>&#x2223;</m:mo>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mrow>
+         <m:mstyle displaystyle=3D"false">
+          <m:mfrac>
+           <m:mn>1</m:mn>
+           <m:mn>2</m:mn>
+          </m:mfrac>
+         </m:mstyle>
+         <m:mo>+</m:mo>
+         <m:mrow>
+          <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+          <m:mi>y</m:mi>
+         </m:mrow>
+        </m:mrow>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+      <m:mo>&#x2223;</m:mo>
+     </m:mrow>
+     <m:mn>2</m:mn>
+    </m:msup>
