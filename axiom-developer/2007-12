+     <m:mi mathvariant=3D"normal">&#x211C;</m:mi>
+     <m:mi>c</m:mi>
+    </m:mrow>
+    <m:mo>,</m:mo>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x211C;</m:mi>
+     <m:mi>d</m:mi>
+    </m:mrow>
+   </m:mrow>
+   <m:mo>&gt;</m:mo>
+   <m:mn>0</m:mn>
+  </m:mrow>
+ </m:math>
+</div>
