+     <m:mi>g</m:mi>
+     <m:mn>6</m:mn>
+    </m:msub>
+    <m:mo>=3D</m:mo>
+    <m:mfrac>
+     <m:mn>52 46819</m:mn>
+     <m:mn>7 52467 96800</m:mn>
+    </m:mfrac>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:msub>
+     <m:mi>g</m:mi>
+     <m:mi>k</m:mi>
+    </m:msub>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:msqrt>
+      <m:mn>2</m:mn>
+     </m:msqrt>
+     <m:msub>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mstyle displaystyle=3D"false">
