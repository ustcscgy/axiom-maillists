+<h4>Multiplication</h4>
+<div align=3D"left">
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>
+    <m:mn>2</m:mn>
+    <m:mi>z</m:mi>
+   </m:mrow>
+   <m:mo>&#x2260;</m:mo>
+   <m:mrow>
+    <m:mn>0</m:mn>
+    <m:mo>,</m:mo>
+    <m:mrow>
+     <m:mo>-</m:mo>
+     <m:mn>1</m:mn>
+    </m:mrow>
+    <m:mo>,</m:mo>
+    <m:mrow>
+     <m:mo>-</m:mo>
+     <m:mn>2</m:mn>
+    </m:mrow>
+    <m:mo>,</m:mo>
+    <m:mi mathvariant=3D"normal">&#x2026;</m:mi>
+   </m:mrow>
+  </m:mrow>
+ </m:math>,
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:mn>2</m:mn>
+       <m:mi>z</m:mi>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:msup>
+      <m:mi>&#x03C0;</m:mi>
+      <m:mrow>
+       <m:mo>-</m:mo>
+       <m:mfrac bevelled=3D"true">
+        <m:mn>1</m:mn>
+        <m:mn>2</m:mn>
+       </m:mfrac>
+      </m:mrow>
+     </m:msup>
