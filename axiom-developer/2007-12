+      <m:mrow>
+       <m:mi>k</m:mi>
+       <m:mo>=3D</m:mo>
+       <m:mn>1</m:mn>
+      </m:mrow>
+      <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+     </m:munderover>
+     <m:msup>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mo>-</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+      <m:mi>k</m:mi>
+     </m:msup>
+     <m:mrow>
+      <m:msup>
+       <m:mi>&#x03C8;</m:mi>
+       <m:mo>&#x2032;</m:mo>
+      </m:msup>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mi>k</m:mi>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:mo>-</m:mo>
+     <m:mfrac>
+      <m:msup>
+       <m:mi>&#x03C0;</m:mi>
+       <m:mn>2</m:mn>
+      </m:msup>
+      <m:mn>8</m:mn>
+     </m:mfrac>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:munderover>
+      <m:mo movablelimits=3D"false">&#x2211;</m:mo>
+      <m:mrow>
+       <m:mi>k</m:mi>
+       <m:mo>=3D</m:mo>
+       <m:mn>1</m:mn>
+      </m:mrow>
+      <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+     </m:munderover>
+     <m:mfrac>
