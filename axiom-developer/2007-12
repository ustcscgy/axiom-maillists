+      <m:mn>1</m:mn>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:msup>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mo>-</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+       <m:mo>)</m:mo>
