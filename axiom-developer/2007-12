+        <m:mi mathvariant=3D"normal">&#x0394;</m:mi>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mrow>
+          <m:msub>
+           <m:mi>t</m:mi>
+           <m:mn>1</m:mn>
+          </m:msub>
+          <m:mo>,</m:mo>
+          <m:mi mathvariant=3D"normal">&#x2026;</m:mi>
+          <m:mo>,</m:mo>
+          <m:msub>
+           <m:mi>t</m:mi>
+           <m:mi>n</m:mi>
+          </m:msub>
+         </m:mrow>
+         <m:mo>)</m:mo>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>|</m:mo>
+      </m:mrow>
+      <m:mrow>
+       <m:mn>2</m:mn>
