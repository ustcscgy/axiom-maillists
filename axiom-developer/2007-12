+--      WriteLine$Lisp "op: "op
       op ^= "SUPERSUB" => _
           "<mtext>Mistake in formatSuperSub: no SUPERSUB1</mtext>"
       #args ^= 1 => "<mtext>Mistake in SuperSub1: #args <> 1</mtext>"
