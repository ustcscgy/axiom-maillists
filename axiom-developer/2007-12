+          <m:mrow>
+           <m:mo>(</m:mo>
+           <m:mrow>
+            <m:mi>a</m:mi>
+            <m:mo>+</m:mo>
+            <m:mrow>
+             <m:mrow>
+              <m:mo>(</m:mo>
+              <m:mrow>
+               <m:mi>n</m:mi>
+               <m:mo>-</m:mo>
+               <m:mi>k</m:mi>
+              </m:mrow>
