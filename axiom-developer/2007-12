+    <m:mrow>
+     <m:mrow>
+      <m:mrow>
+       <m:mo>-</m:mo>
+       <m:mi>&#x03B3;</m:mi>
+      </m:mrow>
+      <m:mo>-</m:mo>
+      <m:mfrac>
+       <m:mn>1</m:mn>
+       <m:mi>z</m:mi>
+      </m:mfrac>
+     </m:mrow>
+     <m:mo>+</m:mo>
+     <m:mrow>
+      <m:munderover>
+       <m:mo movablelimits=3D"false">&#x2211;</m:mo>
+       <m:mrow>
+        <m:mi>k</m:mi>
+        <m:mo>=3D</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
