+<div align=3D"right">
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>=20
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x211C;</m:mi>
+     <m:mi>a</m:mi>
+    </m:mrow>
+    <m:mo>,</m:mo>
+    <m:mrow>
