+       <m:mrow>
+        <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+        <m:mo></m:mo>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mrow>
+          <m:mi>a</m:mi>
+          <m:mo>+</m:mo>
+          <m:mrow>
+           <m:mrow>
+            <m:mo>(</m:mo>
+            <m:mrow>
+             <m:mi>n</m:mi>
