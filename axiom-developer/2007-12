+<p>Here
+</p>
+
+<div align=3D"center">
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:msub>
+      <m:mi>G</m:mi>
+      <m:mn>0</m:mn>
+     </m:msub>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:mi>a</m:mi>
+       <m:mo>,</m:mo>
+       <m:mi>b</m:mi>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mn>1</m:mn>
+   </m:mrow>
+   <m:mo>,</m:mo>
+  </m:mrow>
+ </m:math>
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:msub>
+      <m:mi>G</m:mi>
+      <m:mn>1</m:mn>
+     </m:msub>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:mi>a</m:mi>
+       <m:mo>,</m:mo>
+       <m:mi>b</m:mi>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:mfrac>
+      <m:mn>1</m:mn>
+      <m:mn>2</m:mn>
+     </m:mfrac>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:mi>a</m:mi>
+       <m:mo>-</m:mo>
+       <m:mi>b</m:mi>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
