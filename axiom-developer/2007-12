+         <m:mn>1</m:mn>
+        </m:mrow>
+       </m:msup>
+      </m:mrow>
+      <m:mrow>
+       <m:mn>1</m:mn>
+       <m:mo>-</m:mo>
+       <m:mi>t</m:mi>
+      </m:mrow>
+     </m:mfrac>
+     <m:mspace width=3D"0.2em"/>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x2146;</m:mi>
+      <m:mi>t</m:mi>
+     </m:mrow>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi>&#x03C8;</m:mi>
