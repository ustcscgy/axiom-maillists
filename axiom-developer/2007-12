+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mfrac>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:msub>
+      <m:mo>&#x222B;</m:mo>
+      <m:msub>
