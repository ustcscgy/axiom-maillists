+         <m:mrow>
+          <m:mi>k</m:mi>
+          <m:mo>+</m:mo>
+          <m:mi>z</m:mi>
+         </m:mrow>
+         <m:mo>)</m:mo>
+        </m:mrow>
+       </m:mrow>
+      </m:mfrac>
+     </m:mrow>
+    </m:mrow>
