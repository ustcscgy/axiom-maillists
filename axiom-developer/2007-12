+<h4>Complex Variables</h4>
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mo>|</m:mo>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi>x</m:mi>
+        <m:mo>+</m:mo>
+        <m:mrow>
+         <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+         <m:mi>y</m:mi>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>|</m:mo>
+    </m:mrow>
+    <m:mo>&#x2264;</m:mo>
+    <m:mrow>
+     <m:mo>|</m:mo>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mi>x</m:mi>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>|</m:mo>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mo>|</m:mo>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi>x</m:mi>
+        <m:mo>+</m:mo>
+        <m:mrow>
+         <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+         <m:mi>y</m:mi>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>|</m:mo>
+    </m:mrow>
+    <m:mo>&#x2265;</m:mo>
+    <m:mrow>
+     <m:msup>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mi>sech</m:mi>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mrow>
+          <m:mi>&#x03C0;</m:mi>
+          <m:mi>y</m:mi>
+         </m:mrow>
+         <m:mo>)</m:mo>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+      <m:mfrac bevelled=3D"true">
+       <m:mn>1</m:mn>
+       <m:mn>2</m:mn>
+      </m:mfrac>
+     </m:msup>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mi>x</m:mi>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<div align=3D"right">
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>x</m:mi>
+   <m:mo>&#x2265;</m:mo>
+   <m:mfrac>
+    <m:mn>1</m:mn>
+    <m:mn>2</m:mn>
+   </m:mfrac>
+  </m:mrow>
+ </m:math>
+</div>
+
+<p>For=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>
+    <m:mi>b</m:mi>
+    <m:mo>-</m:mo>
+    <m:mi>a</m:mi>
+   </m:mrow>
+   <m:mo>&#x2265;</m:mo>
+   <m:mn>1</m:mn>
+  </m:mrow>
+ </m:math>,=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>a</m:mi>
+   <m:mo>&#x2265;</m:mo>
+   <m:mn>0</m:mn>
+  </m:mrow>
+ </m:math>, and=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>z</m:mi>
+   <m:mo>=3D</m:mo>
+   <m:mrow>
+    <m:mi>x</m:mi>
+    <m:mo>+</m:mo>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x2148;</m:mi>
+     <m:mi>y</m:mi>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math> with=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>x</m:mi>
+   <m:mo>&gt;</m:mo>
+   <m:mn>0</m:mn>
+  </m:mrow>
+ </m:math>,
+</p>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mo>&#x2223;</m:mo>
+     <m:mfrac>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mrow>
+         <m:mi>z</m:mi>
+         <m:mo>+</m:mo>
+         <m:mi>a</m:mi>
+        </m:mrow>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mrow>
+         <m:mi>z</m:mi>
+         <m:mo>+</m:mo>
+         <m:mi>b</m:mi>
+        </m:mrow>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+     </m:mfrac>
+     <m:mo>&#x2223;</m:mo>
+    </m:mrow>
+    <m:mo>&#x2264;</m:mo>
+    <m:mfrac>
+     <m:mn>1</m:mn>
+     <m:msup>
+      <m:mrow>
+       <m:mo>|</m:mo>
+       <m:mi>z</m:mi>
+       <m:mo>|</m:mo>
+      </m:mrow>
+      <m:mrow>
+       <m:mi>b</m:mi>
+       <m:mo>-</m:mo>
+       <m:mi>a</m:mi>
+      </m:mrow>
+     </m:msup>
+    </m:mfrac>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<p>For=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>x</m:mi>
+   <m:mo>&#x2265;</m:mo>
+   <m:mn>0</m:mn>
+  </m:mrow>
+ </m:math>,
+</p>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mo>|</m:mo>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mi>z</m:mi>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>|</m:mo>
+    </m:mrow>
+    <m:mo>&#x2264;</m:mo>
+    <m:mrow>
+     <m:msup>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mn>2</m:mn>
+        <m:mi>&#x03C0;</m:mi>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+      <m:mfrac bevelled=3D"true">
+       <m:mn>1</m:mn>
+       <m:mn>2</m:mn>
+      </m:mfrac>
+     </m:msup>
+     <m:msup>
+      <m:mrow>
+       <m:mo>|</m:mo>
+       <m:mi>z</m:mi>
+       <m:mo>|</m:mo>
+      </m:mrow>
+      <m:mrow>
+       <m:mi>x</m:mi>
+       <m:mo>-</m:mo>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mfrac bevelled=3D"true">
+         <m:mn>1</m:mn>
+         <m:mn>2</m:mn>
+        </m:mfrac>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+     </m:msup>
+     <m:msup>
+      <m:mi mathvariant=3D"normal">&#x2147;</m:mi>
+      <m:mrow>
+       <m:mo>-</m:mo>
+       <m:mfrac bevelled=3D"true">
+        <m:mrow>
+         <m:mi>&#x03C0;</m:mi>
+         <m:mrow>
+          <m:mo>|</m:mo>
+          <m:mi>y</m:mi>
+          <m:mo>|</m:mo>
+         </m:mrow>=20
+        </m:mrow>
+        <m:mn>2</m:mn>
+       </m:mfrac>
+      </m:mrow>
+     </m:msup>
+     <m:mrow>
+      <m:mi>exp</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:mstyle displaystyle=3D"false">
+         <m:mfrac>
+          <m:mn>1</m:mn>
+          <m:mn>6</m:mn>
+         </m:mfrac>
+        </m:mstyle>
+        <m:msup>
+         <m:mrow>
+          <m:mo>|</m:mo>
