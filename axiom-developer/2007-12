+       </m:msub>
+      </m:mrow>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mfrac>
+     <m:mrow>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:msub>
+         <m:mi>z</m:mi>
+         <m:mn>1</m:mn>
+        </m:msub>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:msub>
+         <m:mi>z</m:mi>
+         <m:mn>2</m:mn>
+        </m:msub>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+      <m:mi mathvariant=3D"normal">&#x22EF;</m:mi>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:msub>
+         <m:mi>z</m:mi>
+         <m:mrow>
+          <m:mi>n</m:mi>
+          <m:mo>+</m:mo>
+          <m:mn>1</m:mn>
+         </m:mrow>
+        </m:msub>
+        <m:mo>)</m:mo>
+       </m:mrow>
+      </m:mrow>
+     </m:mrow>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mrow>
+        <m:msub>
+         <m:mi>z</m:mi>
+         <m:mn>1</m:mn>
+        </m:msub>
+        <m:mo>+</m:mo>
+        <m:msub>
+         <m:mi>z</m:mi>
+         <m:mn>2</m:mn>
+        </m:msub>
+        <m:mo>+</m:mo>
+        <m:mi mathvariant=3D"normal">&#x2026;</m:mi>
+        <m:mo>+</m:mo>
+        <m:msub>
+         <m:mi>z</m:mi>
+         <m:mrow>
+          <m:mi>n</m:mi>
+          <m:mo>+</m:mo>
+          <m:mn>1</m:mn>
+         </m:mrow>
+        </m:msub>
+       </m:mrow>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+    </m:mfrac>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<h5>Selberg-type Integrals</h5>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi mathvariant=3D"normal">&#x0394;</m:mi>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:msub>
+        <m:mi>t</m:mi>
+        <m:mn>1</m:mn>
+       </m:msub>
+       <m:mo>,</m:mo>
+       <m:msub>
+        <m:mi>t</m:mi>
+        <m:mn>2</m:mn>
+       </m:msub>
+       <m:mo>,</m:mo>
+       <m:mi mathvariant=3D"normal">&#x2026;</m:mi>
+       <m:mo>,</m:mo>
+       <m:msub>
+        <m:mi>t</m:mi>
+        <m:mi>n</m:mi>
+       </m:msub>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:munder>
+      <m:mo movablelimits=3D"false">&#x220F;</m:mo>
+      <m:mrow>
+       <m:mn>1</m:mn>
+       <m:mo>&#x2264;</m:mo>
+       <m:mi>j</m:mi>
+       <m:mo>&lt;</m:mo>
+       <m:mi>k</m:mi>
+       <m:mo>&#x2264;</m:mo>
+       <m:mi>n</m:mi>
+      </m:mrow>
+     </m:munder>
+     <m:mo>(</m:mo>
+     <m:mrow>
+      <m:msub>
+       <m:mi>t</m:mi>
+       <m:mi>j</m:mi>
+      </m:msub>
+      <m:mo>-</m:mo>
+      <m:msub>
+       <m:mi>t</m:mi>
+       <m:mi>k</m:mi>
+      </m:msub>
+     </m:mrow>
+     <m:mo>)</m:mo>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<p>Then
+</p>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:msub>
+      <m:mo>&#x222B;</m:mo>
+      <m:msup>
+       <m:mrow>
+        <m:mo>[</m:mo>
+        <m:mrow>
+         <m:mn>0</m:mn>
+         <m:mo>,</m:mo>
+         <m:mn>1</m:mn>
+        </m:mrow>
+        <m:mo>]</m:mo>
+       </m:mrow>
+       <m:mi>n</m:mi>
+      </m:msup>
+     </m:msub>
+     <m:msub>
+      <m:mi>t</m:mi>
+      <m:mn>1</m:mn>
+     </m:msub>
+     <m:msub>
+      <m:mi>t</m:mi>
+      <m:mn>2</m:mn>
+     </m:msub>
+     <m:mi mathvariant=3D"normal">&#x22EF;</m:mi>
+     <m:msub>
+      <m:mi>t</m:mi>
+      <m:mi>m</m:mi>
+     </m:msub>
+     <m:msup>
+      <m:mrow>
+       <m:mo>|</m:mo>
+       <m:mrow>
+        <m:mi mathvariant=3D"normal">&#x0394;</m:mi>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mrow>
+          <m:msub>
+           <m:mi>t</m:mi>
+           <m:mn>1</m:mn>
+          </m:msub>
+          <m:mo>,</m:mo>
+          <m:mi mathvariant=3D"normal">&#x2026;</m:mi>
+          <m:mo>,</m:mo>
+          <m:msub>
+           <m:mi>t</m:mi>
+           <m:mi>n</m:mi>
+          </m:msub>
+         </m:mrow>
+         <m:mo>)</m:mo>
+        </m:mrow>
+       </m:mrow>
+       <m:mo>|</m:mo>
+      </m:mrow>
+      <m:mrow>
+       <m:mn>2</m:mn>
+       <m:mi>c</m:mi>
+      </m:mrow>
+     </m:msup>
+     <m:mrow>
+      <m:munderover>
+       <m:mo movablelimits=3D"false">&#x220F;</m:mo>
+       <m:mrow>
+        <m:mi>k</m:mi>
+        <m:mo>=3D</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+       <m:mi>n</m:mi>
+      </m:munderover>
+      <m:msubsup>
+       <m:mi>t</m:mi>
+       <m:mi>k</m:mi>
+       <m:mrow>
+        <m:mi>a</m:mi>
+        <m:mo>-</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+      </m:msubsup>
+      <m:msup>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mrow>
+         <m:mn>1</m:mn>
+         <m:mo>-</m:mo>
+         <m:msub>
+          <m:mi>t</m:mi>
+          <m:mi>k</m:mi>
+         </m:msub>
+        </m:mrow>
+        <m:mo>)</m:mo>
+       </m:mrow>
+       <m:mrow>
+        <m:mi>b</m:mi>
+        <m:mo>-</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+      </m:msup>
+      <m:mrow>
+       <m:mi mathvariant=3D"normal">&#x2146;</m:mi>
+       <m:msub>
+        <m:mi>t</m:mi>
+        <m:mi>k</m:mi>
+       </m:msub>
+      </m:mrow>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:mfrac>
+      <m:mn>1</m:mn>
+      <m:msup>
+       <m:mrow>
+        <m:mo>(</m:mo>
+        <m:mrow>
+         <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+         <m:mrow>
+          <m:mo>(</m:mo>
+          <m:mrow>
+           <m:mn>1</m:mn>
+           <m:mo>+</m:mo>
+           <m:mi>c</m:mi>
+          </m:mrow>
+          <m:mo>)</m:mo>
+         </m:mrow>
+        </m:mrow>
+        <m:mo>)</m:mo>
+       </m:mrow>
+       <m:mi>n</m:mi>
+      </m:msup>
+     </m:mfrac>
+     <m:mrow>
+      <m:munderover>
+       <m:mo movablelimits=3D"false">&#x220F;</m:mo>
+       <m:mrow>
+        <m:mi>k</m:mi>
+        <m:mo>=3D</m:mo>
+        <m:mn>1</m:mn>
+       </m:mrow>
+       <m:mi>m</m:mi>
+      </m:munderover>
+      <m:mfrac>
+       <m:mrow>
+        <m:mi>a</m:mi>
+        <m:mo>+</m:mo>
+        <m:mrow>
+         <m:mrow>
+          <m:mo>(</m:mo>
+          <m:mrow>
+           <m:mi>n</m:mi>
+           <m:mo>-</m:mo>
+           <m:mi>k</m:mi>
+          </m:mrow>
+          <m:mo>)</m:mo>
+         </m:mrow>
+         <m:mi>c</m:mi>
+        </m:mrow>
+       </m:mrow>
+       <m:mrow>
+        <m:mi>a</m:mi>
+        <m:mo>+</m:mo>
+        <m:mi>b</m:mi>
+        <m:mo>+</m:mo>
+        <m:mrow>
+         <m:mrow>
+          <m:mo>(</m:mo>
+          <m:mrow>
+           <m:mrow>
+            <m:mn>2</m:mn>
+            <m:mi>n</m:mi>
+           </m:mrow>
+           <m:mo>-</m:mo>
+           <m:mi>k</m:mi>
+           <m:mo>-</m:mo>
+           <m:mn>1</m:mn>
+          </m:mrow>
+          <m:mo>)</m:mo>
+         </m:mrow>
+         <m:mi>c</m:mi>
+        </m:mrow>
+       </m:mrow>
+      </m:mfrac>
+      <m:mrow>
+       <m:munderover>
+        <m:mo movablelimits=3D"false">&#x220F;</m:mo>
+        <m:mrow>
+         <m:mi>k</m:mi>
+         <m:mo>=3D</m:mo>
+         <m:mn>1</m:mn>
+        </m:mrow>
+        <m:mi>n</m:mi>
+       </m:munderover>
+       <m:mfrac>
+        <m:mrow>
+         <m:mrow>
