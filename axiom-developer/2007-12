+      <m:mi>a</m:mi>
+      <m:mi>k</m:mi>
+     </m:msub>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:munderover>
+      <m:mo movablelimits=3D"false">&#x2211;</m:mo>
+      <m:mrow>
+       <m:mi>k</m:mi>
+       <m:mo>=3D</m:mo>
