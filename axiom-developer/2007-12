+       <m:mi>t</m:mi>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>+</m:mo>
+     <m:mrow>
+      <m:munderover>
+       <m:mo movablelimits=3D"false">&#x2211;</m:mo>
+       <m:mrow>
+        <m:mi>k</m:mi>
+        <m:mo>=3D</m:mo>
+        <m:mn>0</m:mn>
+       </m:mrow>
+       <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+      </m:munderover>
+      <m:mfrac>
+       <m:msup>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mrow>
+          <m:mo>-</m:mo>
+          <m:mn>1</m:mn>
+         </m:mrow>
+         <m:mo>)</m:mo>
+        </m:mrow>
+        <m:mi>k</m:mi>
+       </m:msup>
+       <m:mrow>
+        <m:mrow>
