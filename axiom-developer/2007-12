+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:msup>
+     <m:mrow>
+      <m:mo>|</m:mo>
+      <m:mfrac>
+       <m:mrow>
