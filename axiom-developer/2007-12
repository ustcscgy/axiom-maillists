+<<standard head>>
+ </head>
+ <body>
+<<page head>>
+  <div align=3D"center">
+   <a href=3D"http://dlmf.nist.gov">
+    Digital Library of Mathematical Functions
+   </a><br/>
+   The Gamma Function -- Barnes G-Function (Double Gamma Function)
+  </div>
+  <hr/>
+<h3>Barnes=20
+ <m:math display=3D"inline">
+  <m:mi mathvariant=3D"bold-italic">G</m:mi>
+ </m:math>-Function (Double Gamma Function)
+</h3>
+
+<div align=3D"center">
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:mi>G</m:mi>
+     <m:mrow>
+      <m:mo>(</m:mo>
+      <m:mrow>
+       <m:mi>z</m:mi>
+       <m:mo>+</m:mo>
+       <m:mn>1</m:mn>
+      </m:mrow>
+      <m:mo>)</m:mo>
+     </m:mrow>
+    </m:mrow>
+    <m:mo>=3D</m:mo>
+    <m:mrow>
+     <m:mrow>
+      <m:mi mathvariant=3D"normal">&#x0393;</m:mi>
+      <m:mrow>
+       <m:mo>(</m:mo>
+       <m:mi>z</m:mi>
+       <m:mo>)</m:mo>
+      </m:mrow>
+     </m:mrow>
+     <m:mspace width=3D"0.2em"/>
+     <m:mrow>
+      <m:mi>G</m:mi>
+      <m:mrow>
