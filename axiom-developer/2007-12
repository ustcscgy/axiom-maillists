+      </m:mrow>
+      <m:mo>-</m:mo>
+      <m:mrow>
+       <m:mi>ln</m:mi>
+       <m:mi>q</m:mi>
+      </m:mrow>
+      <m:mo>-</m:mo>
+      <m:mrow>
+       <m:mfrac>
+        <m:mi>&#x03C0;</m:mi>
+        <m:mn>2</m:mn>
+       </m:mfrac>
+       <m:mrow>
+        <m:mi>cot</m:mi>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mfrac>
+          <m:mrow>
+           <m:mi>&#x03C0;</m:mi>
+           <m:mi>p</m:mi>
+          </m:mrow>
+          <m:mi>q</m:mi>
+         </m:mfrac>
+         <m:mo>)</m:mo>
+        </m:mrow>
+       </m:mrow>
+      </m:mrow>
+     </m:mrow>
+     <m:mo>+</m:mo>
+     <m:mrow>
+      <m:mfrac>
+       <m:mn>1</m:mn>
+       <m:mn>2</m:mn>
+      </m:mfrac>
+      <m:mrow>
+       <m:munderover>
+        <m:mo movablelimits=3D"false">&#x2211;</m:mo>
+        <m:mrow>
+         <m:mi>k</m:mi>
+         <m:mo>=3D</m:mo>
+         <m:mn>1</m:mn>
+        </m:mrow>
+        <m:mrow>
+         <m:mi>q</m:mi>
+         <m:mo>-</m:mo>
+         <m:mn>1</m:mn>
+        </m:mrow>
+       </m:munderover>
+       <m:mrow>
+        <m:mi>cos</m:mi>
+        <m:mrow>
+         <m:mo>(</m:mo>
+         <m:mfrac>
+          <m:mrow>
+           <m:mn>2</m:mn>
