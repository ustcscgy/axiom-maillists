+        </m:msup>
+       </m:mfrac>
+      </m:mrow>
+     </m:mrow>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</div>
+
+<p>As=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mi>z</m:mi>
+   <m:mo>&#x2192;</m:mo>
+   <m:mi mathvariant=3D"normal">&#x221E;</m:mi>
+  </m:mrow>
+ </m:math> in=20
+ <m:math display=3D"inline">
+  <m:mrow>
+   <m:mrow>
+    <m:mo>|</m:mo>
+    <m:mrow>
+     <m:mi>ph</m:mi>
+     <m:mspace width=3D"0.2em"/>
+     <m:mi>z</m:mi>
+    </m:mrow>
+    <m:mo>|</m:mo>
+   </m:mrow>
+   <m:mo>&#x2264;</m:mo>
+   <m:mrow>
+    <m:mrow>
+     <m:mi>&#x03C0;</m:mi>
+     <m:mo>-</m:mo>
+     <m:mi>&#x03B4;</m:mi>
+    </m:mrow>
+    <m:mrow>
+     <m:mo>(</m:mo>
+     <m:mrow>
+      <m:none/>
+      <m:mo>&lt;</m:mo>
+      <m:mi>&#x03C0;</m:mi>
+     </m:mrow>
+     <m:mo>)</m:mo>
+    </m:mrow>
+   </m:mrow>
+  </m:mrow>
+ </m:math>
+</p>
+
+<div align=3D"center">
+ <m:math display=3D"block">
+  <m:mrow>
+   <m:mrow>
+    <m:mrow>
+     <m:msup>
+      <m:mi>&#x03C8;</m:mi>
+      <m:mo>&#x2032;</m:mo>
+     </m:msup>
+     <m:mrow>
+      <m:mo>(</m:mo>
