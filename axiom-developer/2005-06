Changes http://page.axiom-developer.org/zope/mathaction/ListToMatrix/diff
--
\begin{axiom}
A:=matrix[[cos(x)-L,-sin(x)],[sin(x),cos(x)-L]]
D:=matrix[[sqrt(-1)*sin(x)+cos(x)],[-sqrt(-1)*sin(x)+cos(x)]]
v:=matrix[[v11],[v12]]
A*v
D(1,1)*v
solve(A*v=D(1,1)*v,v11)
\end{axiom}
